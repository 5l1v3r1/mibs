qosDscpDot1PAlignment OBJECT IDENTIFIER ::= {qosMIB 8}

qosDscpToDot1PAlignmentTable OBJECT-TYPE
        SYNTAX  SEQUENCE OF QosDscpToDot1PAlignmentEntry
        ACCESS	not-accessible
	STATUS	mandatory
	DESCRIPTION
	"This object contains a table that defines the mapping from DSCP
         value to the .1P field of the VLAN tag. The row index of the table
         is the ingress DSCP value (0..63) while the value is the .1P(0..7)."
::= { qosDscpDot1PAlignment 1}

qosDscpToDot1PAlignmentEntry OBJECT-TYPE
	SYNTAX QosDscpToDot1PAlignmentEntry
	ACCESS not-accessible
	STATUS	mandatory
	DESCRIPTION
	"This table contains the DSCP value to 802.1P traffic class mapping
         information. There are 64 rows, each containing one .1P value (0..7)"
	INDEX {qosDscpToDot1PAlignmentDscp }
	::= {qosDscpToDot1PAlignmentTable  1}

QosDscpToDot1PAlignmentEntry ::=
	SEQUENCE {	qosDscpToDot1PAlignmentDscp      INTEGER,	
			qosDscpToDot1PAlignmentDot1P     INTEGER
		 }

qosDscpToDot1PAlignmentDscp OBJECT-TYPE
	SYNTAX	INTEGER(0..63)
	ACCESS	not-accessible
	STATUS mandatory
	DESCRIPTION
	    "This field is used to index the DSCP to Dot1P alignment table.
             The lengths of the table is 64 (fixed)"
::= {  qosDscpToDot1PAlignmentEntry 1 }

qosDscpToDot1PAlignmentDot1P OBJECT-TYPE
	SYNTAX	INTEGER(0..7)
	ACCESS	read-write
	STATUS mandatory
	DESCRIPTION
	"This object encodes the Dot1P (0..7) into which
	frames coming in with the DSCP value equal to the
	index of the row in this table.
	E.g., if row 63 contains value 7, that means that frames
	with DSCP value equal to 63 will be encoded to .1P value 7"
::= {  qosDscpToDot1PAlignmentEntry 2 }

qosDscpToDot1PAlignmentL3Traffic OBJECT-TYPE
	SYNTAX INTEGER	{
				true (1),
				false (2)
			}
	ACCESS read-write
	STATUS mandatory
	DESCRIPTION
        "setting this object to true will do the DSCP to P-bits
         alignment for all the L3 forwarding traffic"
        DEFVAL {false}
::= {	qosDscpDot1PAlignment 2}

    qosPccRules OBJECT IDENTIFIER ::= { qosMIB 9 }

    pccRuleTable OBJECT-TYPE
        SYNTAX          SEQUENCE OF PccRuleEntry
        ACCESS          not-accessible
        STATUS          mandatory
        DESCRIPTION
             "The table is partitioned per userLineId and sub-partitioned
              per userFwdPortId. 
              Each table row is indexed by pccRuleIndex.
              Each such table row contains the parameters of a PCC Rule obtained
              via e.g. the DIAMETER Gxr Interface.
        ALCATEL NOTE:
             TABLESIZE: depends on the number of User Lines,
                  multiplied by the average number of PCC Rules per User Line.
                  The latter is about 0.15 PCC Rules, derived from:
                  average 1.5 set-top box per household, and
                  average 10% of the streaming traffic is unicast video (VoD). "
        ::= { qosPccRules 1 }

    pccRuleEntry OBJECT-TYPE
        SYNTAX          PccRuleEntry
        ACCESS          not-accessible
        STATUS          mandatory
        DESCRIPTION
             "Rows are created automatically when a PCC rule is accepted and
              installed by the DSLAM after being downloaded by a Policy and
              Charging Rule Function (PCRF server). "
        INDEX { userLineId, userFwdPortId, pccRuleIndex }
        ::= { pccRuleTable 1 }

    PccRuleEntry ::= SEQUENCE {
        userLineId           InterfaceIndex,
        userFwdPortId        InterfaceIndex,
        pccRuleIndex                INTEGER,
        pccRuleName            OCTET STRING,
        pccRuleMaxDownRate          INTEGER,
        pccRulePreEmptPriority       Uint32,
        pccRulePreEmpVulnerability  INTEGER
        }

    userLineId OBJECT-TYPE
        SYNTAX          InterfaceIndex
        ACCESS          read-only
        STATUS          mandatory
        DESCRIPTION
             "The userLineId identifies a ADSL-port index, a VDSL-port index, 
              or a native Ethernet-port index.
              In case the PCC Rule is installed via the DIAMETER Gxr Interface, the
              userLine Id must match with a rcSessionUserLineId in the DIAMETER
              MIB.
        ALCATEL NOTE:
             ACCESS:    NA
             USAGE:     NA
             PERSIST:   NA
             INSRVMOD:  NA
             RANGE:     NA
             DEFVALUE:  NA
             UNITS:     NA
             SPARSE:    NO "
        ::= { pccRuleEntry 1 }

    userFwdPortId OBJECT-TYPE
        SYNTAX          InterfaceIndex
        ACCESS          read-only
        STATUS          mandatory
        DESCRIPTION
             "The userFwdPortId identify a VLAN port or a PPPoE client port, so
              in general the access port to the forwarding service.
              The value is the ifIndex that matches with the circuit id in the
              Gxr's Bearer-Identifier AVP.
        ALCATEL NOTE:
             ACCESS:    NA
             USAGE:     NA
             PERSIST:   NA
             INSRVMOD:  NA
             RANGE:     NA
             DEFVALUE:  NA
             UNITS:     NA
             SPARSE:    NO "
        ::= { pccRuleEntry 2 }

    pccRuleIndex OBJECT-TYPE
        SYNTAX          INTEGER
        ACCESS          read-only
        STATUS          mandatory
        DESCRIPTION
             "An index to distinguish between different PCC rules.
        ALCATEL NOTE:
             ACCESS:    NA
             USAGE:     NA
             PERSIST:   NA
             INSRVMOD:  NA
             RANGE:     NA
             DEFVALUE:  NA
             UNITS:     NA
             SPARSE:    NO "
        ::= { pccRuleEntry 3 }

    pccRuleName OBJECT-TYPE
        SYNTAX          OCTET STRING (SIZE(0..32))
        ACCESS          read-only
        STATUS          mandatory
        DESCRIPTION
             "The name of the PCC rule.  
              The name is unique with an RC session, i.e. per User Line.
        ALCATEL NOTE:
             ACCESS:    NA
             USAGE:     NA
             PERSIST:   NA
             INSRVMOD:  NA
             RANGE:     NA
             DEFVALUE:  NA
             UNITS:     NA
             SPARSE:    NO "
        ::= { pccRuleEntry 4 }

    pccRuleMaxDownRate OBJECT-TYPE
        SYNTAX          INTEGER
        ACCESS          read-only
        STATUS          mandatory
        DESCRIPTION
             "The pccRuleMaxDownRate defines the maximum bit rate to be reserved
              for the downlink direction. 
        ALCATEL NOTE:
             ACCESS:    NA
             USAGE:     NA
             PERSIST:   NA
             INSRVMOD:  NA
             RANGE:     NA
             DEFVALUE:  NA
             UNITS:     Kilobits/sec
             SPARSE:    NO "
        ::= { pccRuleEntry 5 }

    pccRulePreEmptPriority OBJECT-TYPE
        SYNTAX          Uint32
        ACCESS          read-only
        STATUS          mandatory
        DESCRIPTION
             "The value of pccRulePreEmptPriority is used to decide which PCC rules
              to terminate in the event of a bandwidth drop of the User Line, 
              whilst the line remains operational.
              The highest level of priority is specified by the value 1.
        ALCATEL NOTE:
             ACCESS:    NA
             USAGE:     NA
             PERSIST:   NA
             INSRVMOD:  NA
             RANGE:     1..15
             DEFVALUE:  NA
             UNITS:     NA
             SPARSE:    NO "
        ::= { pccRuleEntry 6 }

    pccRulePreEmpVulnerability OBJECT-TYPE
        SYNTAX          INTEGER {
                                enabled(1),
                                disabled(2)
                                }
        ACCESS          read-only
        STATUS          mandatory
        DESCRIPTION
             "When pccRulePreEmpVulnerability is enabled, it is allowed to
              consider this rule for termination in the event of a bandwidth drop
              of the User Line, whilst the line remains operational.
        ALCATEL NOTE:
             ACCESS:    NA
             USAGE:     NA
             PERSIST:   NA
             INSRVMOD:  NA
             RANGE:     NA
             DEFVALUE:  NA
             UNITS:     NA
             SPARSE:    NO "
        ::= { pccRuleEntry 7 }

    pccRuleEthIpFactor OBJECT-TYPE
        SYNTAX          Uint32
        ACCESS          read-write
        STATUS          mandatory
        DESCRIPTION
             "The rates in PCC rules are IP-rates, while ISAM RAC calculations
              are done with Eth-rates.
              The value pccRuleEthIpFactor gives in percentages the conversion
              factor IP-to-Ethernet. So the frame net bandwidth available on a
              DSL link for transmitting Ethernet frames equals the IP-rate
              multiplied by pccRuleEthIpFactor/100. 
              Ethernet overhead is about 20 bytes per IP packet. Then for 
              a stream of 1Kbyte IP packets, the Ehernet overhead is 2 percent,
              and so the pccRuleEthIpFactor is 102 percent.
              This is taken as default value.
        ALCATEL NOTE:
             ACCESS:    NA
             USAGE:     NA
             PERSIST:   YES
             INSRVMOD:  YES
             RANGE:     NA
             DEFVALUE:  102
             UNITS:     percentage
             SPARSE:    NO "
        ::= { qosPccRules 2 }

END

